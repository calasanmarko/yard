{
  "name": "@kranehq/yard",
  "version": "0.0.5",
  "description": "CLI for routing local dev servers to stable *.localhost hostnames",
  "keywords": [
    "yard",
    "localhost",
    "dev-proxy",
    "development",
    "cli",
    "vite"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calasanmarko/yard.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/calasanmarko/yard/issues"
  },
  "homepage": "https://github.com/calasanmarko/yard/tree/main/packages/cli",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": {
    "yard": "dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "bun run build"
  }
}
